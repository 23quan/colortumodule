<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data></data>

    <LinearLayout
        android:id="@+id/sign_itemview"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_110"
        android:layout_marginRight="@dimen/dimen_15"
        android:layout_marginBottom="@dimen/dimen_15"
        android:background="@drawable/base_blue5_bg"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/sign_icon"
            android:layout_width="@dimen/dimen_35"
            android:layout_height="@dimen/dimen_35"
            android:scaleType="fitXY"></ImageView>

        <TextView
            android:id="@+id/sign_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_5"
            android:textColor="@color/base_white1"
            android:textSize="@dimen/size_25"></TextView>
    </LinearLayout>
</layout>