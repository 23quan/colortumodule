<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置背景色（灰色） -->
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/base_blue18"></solid>
        </shape>
    </item>
    <!-- 设置进度条颜色（蓝色） -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/base_blue19"
                    android:startColor="@color/base_blue19"></gradient>

            </shape>
        </clip>
    </item>
</layer-list>
