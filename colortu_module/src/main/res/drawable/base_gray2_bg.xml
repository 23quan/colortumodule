<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/base_transparent" />
            <corners android:radius="@dimen/dimen_10" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <corners android:radius="@dimen/dimen_10" />
            <solid android:color="@color/base_transparent" />
            <stroke android:width="@dimen/dimen_2" android:color="@color/base_gray3" />
        </shape>
    </item>
</selector>